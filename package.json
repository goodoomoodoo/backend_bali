{
  "name": "bali",
  "version": "1.0.0",
  "description": "Backend project to Assembly IDE",
  "main": "index.js",
  "scripts": {
    "start": "npm run local-dev",
    "local-dev": "ts-node index.ts --init config/local.dev.config.yaml",
    "remote-dev": "ts-node index.ts --init config/remote.dev.config.yaml",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "test": "mocha -r ts-node/register test/**/*Test.ts"
  },
  "keywords": [
    "Nodejs",
    "REST"
  ],
  "author": "Arvin Lin",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/js-yaml": "^3.12.5",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.13",
    "@types/yargs": "^15.0.11",
    "gts": "^3.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "js-yaml": "^3.14.1",
    "mocha": "^8.2.1",
    "morgan": "^1.10.0",
    "winston": "^3.3.3",
    "yargs": "^16.2.0"
  }
}
